#active present participles:
#with suffix -ся-
REMOVE:WPTCP-A2.1 A IF (0 A)(0 PresActv + V/Ref + (<W=MAX>))(NOT 0 Adv)(NOT 0 N) ;
#transitive use
REMOVE:WPTCP-A2.2 A IF (0 A)(0 PresActv + TV + (<W=MAX>))(NOT 0 Adv)(NOT 0 N) ;

