REMOVE:WPTCP-A4.4 A IF (0 A)(0 Ptcp + PrsPss + V/PssPrs + (<W=MAX>))(NOT 0 Adv)(NOT 0 N)(1 N + Ins OR Pron + Ins)  ;
#general use
REMOVE:WPTCP-A4.5 A IF (0 A)(0 Ptcp + PrsPss + (<W=MAX>))(NOT 0 Adv)(NOT 0 N)(1 N + Ins OR Pron + Ins)  ; 

#passive present participles without complement:
#imperfective transitive
REMOVE:WPTCP-A4.6 A IF (0 A)(0 Ptcp + PrsPss + Impf + TV + (<W=MAX>))(NOT 0 Adv)(NOT 0 N) ; 
#imperfective 
REMOVE:WPTCP-A4.7 A IF (0 A)(0 Ptcp + PrsPss + Impf + (<W=MAX>))(NOT 0 Adv)(NOT 0 N) ; 
#transitive
REMOVE:WPTCP-A4.8 A IF (0 A)(0 Ptcp + PrsPss + TV + (<W=MAX>))(NOT 0 Adv)(NOT 0 N) ;
#suffix -мый-
REMOVE:WPTCP-A4.9 A IF (0 A)(0 Ptcp + PrsPss + V/PssPrs + (<W=MAX>))(NOT 0 Adv)(NOT 0 N)(1 N + Ins OR Pron + Ins)  ;

