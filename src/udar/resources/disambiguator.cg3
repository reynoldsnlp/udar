#Predicative use:
#safe weighted rule
REMOVE:WPTCP-A-Pred A IF (0 A)(0 Ptcp + Pred + (<W=MAX>))(NOT 0 Adv)(NOT 0 N) ;
#unweighted rule
REMOVE:PTCP-A-Pred A IF (0 A)(0 Ptcp + Pred)(NOT 0 Adv)(NOT 0 N) ;

#negation particle
REMOVE:WPTCP-Ne-V A IF (0 A)(0 Ptcp + (<W=MIN>))(NOT 0 Adv)(NOT 0 N)(-1 Ne) ;

##################### + adjectivization => REMOVE V #####################
#Constraints are based on morphological properties (+ some context) discouraging participial uses 

#Composite patterns: + adjectivization

#active present adjectivized participles:
REMOVE:WPTCP-V4.1 V IF (0 A)(0 Ptcp + PrsAct + IV + (<W=MIN>))(NOT 0 Adv)(NOT 0 N)((NOT 1 S-BOUNDARY) OR (NOT 1 N + Dat) OR (NOT 0 Pr - Pr/Iz)) ;
REMOVE:WPTCP-V4.2 V IF (0 A)(0 Ptcp + PrsAct + Impf + (<W=MIN>))(NOT 0 Adv)(NOT 0 N)((NOT 1 S-BOUNDARY) OR (NOT 1 N + Dat) OR (NOT 0 Pr - Pr/Iz)) ;
#скучающий

#active past adjectivized participles without complement: e.g.: отживший, застывший:
REMOVE:WPTCP-V5.1 V IF (0 A)(0 Ptcp + PstAct  + V/PstActPerf + (<W=MAX>))(NOT 0 Adv)(NOT 0 N) ;
#suffixes -вш-, -ш-
REMOVE:WPTCP-V5.2 V IF (0 A)(0 Ptcp + PstAct + Perf + (<W=MIN>))(NOT 0 Adv)(NOT 0 N) ;
REMOVE:WPTCP-V5.3 V IF (0 A)(0 Ptcp + PstAct + Perf + IV + (<W=MIN>))(NOT 0 Adv)(NOT 0 N) ;

#passive past adjectivized participles with complement: 
REMOVE:WPTCP-V6.1 V IF (0 A)(0 Ptcp + PstPss + Perf + (<W=MIN>))(NOT 0 Adv)(NOT 0 N)(1 N + NOINS OR Pron + NOINS) ;
#уважаемый, воображаемый

#present passive adjectivized participles without complement: 
REMOVE:WPTCP-V6.2 V IF (0 A)(NOT 0 Adv)(NOT 0 N)(0 Ptcp + PrsPss + Perf + (<W=MIN>)) ;
#уважаемый, воображаемый

#passive past adjectivized participles without complement (non-predicative use): 
#perfective use
REMOVE:WPTCP-V7.2 V IF (0 A)(0 Ptcp + PstPss + Perf - Pred + (<W=MIN>))(NOT 0 Adv)(NOT 0 N) ;
#use with suffixes -нн/т-
REMOVE:WPTCP-V7.3 V IF (0 A)(0 Ptcp + PstPss + Perf + V/PstPssPerf - Pred + (<W=MIN>))(NOT 0 Adv)(NOT 0 N) ;
#single use
REMOVE:WPTCP-V7.1 V IF (0 A)(0 Ptcp + PstPss - Pred + (<W=MIN>))(NOT 0 Adv)(NOT 0 N) ;

